{
  "name": "uniapp-request-sdk",
  "version": "1.4.1",
  "description": "用于uniapp小程序的请求库的sdk",
  "main": "dist/index.umd.js",
  "module": " dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rimraf dist typings && tsc --emitDeclarationOnly && cross-env NODE_ENV=development rollup -c --watch",
    "build": "rimraf dist typings && tsc --emitDeclarationOnly && cross-env NODE_ENV=production rollup -c",
    "prepublish": "npm run build",
    "c": "git-cz"
  },
  "author": "yaojun",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@dcloudio/types": "^2.2.2",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/node": "^14.10.1",
    "@types/uni-app": "^1.4.4",
    "@typescript-eslint/parser": "^5.54.1",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.2",
    "cz-lerna-changelog": "^2.0.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.3.0",
    "patch-package": "^6.5.1",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.11",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clean": "^1.0.0",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "typescript": "^4.0.2"
  },
  "buildOptions": {
    "name": "UniappRequestSdk",
    "formats": [
      "umd",
      "esm"
    ]
  }
}
